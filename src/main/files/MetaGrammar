# Meta Grammar

grammar:
    | [NEWLINE] rule+
rule:
    | NAME ':' NEWLINE '|' alt_list NEWLINE
alt_list:
    | sequence ([NEWLINE] '|' sequence)*
sequence:
    | primary+
primary:
    | delimited
    | item '*'
    | item '+'
    | item
item:
    | group
    | optional
    | NAME
    | STRING
group:
    | '(' alt_list ')'
optional:
    | '[' alt_list ']'
delimited:
    | STRING '.' item '+'
